{
  "name": "electron-vue-starter",
  "author": "aki",
  "description": "electron vue starter",
  "email": "earlfx@163.com",
  "private": true,
  "version": "0.1.2",
  "main": "dist/.electron/main.js",
  "scripts": {
    "dev": "kill-port 3000 && concurrently -k \"vite\" \"wait-on tcp:3000 && npm-run-all watch\"",
    "watch": "tsc-watch --noClear -p tsconfig.electron.json --onSuccess \"npm-run-all start\"",
    "start": "cross-env NODE_ENV=development run-electron ./dist/.electron/main.js",
    "render:dev": "vite",
    "main:dev": "electron .",
    "build": "npm-run-all build:vue build:tsc build:all",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:tsc": "tsc -p tsconfig.electron.json",
    "build:all": "electron-builder --publish always",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.32",
    "@vueuse/core": "^9.1.0",
    "@vueuse/electron": "^9.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "^1.0.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "electron": "^17.4.11",
    "electron-builder": "^23.3.3",
    "electron-devtools-vendor": "^1.1.0",
    "electron-rebuild": "^3.2.9",
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "run-electron": "^1.0.0",
    "tailwindcss": "^3.1.7",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vue-tsc": "^0.39.4",
    "wait-on": "^6.0.1"
  }
}
