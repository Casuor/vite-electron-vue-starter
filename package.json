{
  "name": "electron-vue-starter",
  "author": "aki",
  "description": "electron vue starter",
  "email": "earlfx@163.com",
  "version": "0.1.2",
  "license": "ISC",
  "main": "dist/.electron/main.js",
  "engines": {
    "node": "16.14.2",
    "vscode": "latest"
  },
  "scripts": {
    "dev": "kill-port 3000 && concurrently -k \"vite\" \"wait-on tcp:3000 && npm-run-all watch\"",
    "watch": "tsc-watch --noClear -p tsconfig.electron.json --onSuccess \"npm-run-all start\"",
    "start": "cross-env NODE_ENV=development run-electron ./dist/.electron/main.js",
    "render:dev": "vite",
    "main:dev": "run-electron .",
    "clear": "rimraf dist build",
    "build": "npm-run-all build:vue build:tsc build:all",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:tsc": "tsc -p tsconfig.electron.json",
    "build:all": "electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@headlessui/vue": "^1.7.4",
    "@heroicons/vue": "^2.0.13",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-vendor": "^1.1.0",
    "electron-rebuild": "^3.2.9",
    "is-electron": "^2.2.1",
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "rimraf": "^3.0.2",
    "run-electron": "^1.0.0",
    "tailwindcss": "^3.2.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue-tsc": "^1.0.9",
    "wait-on": "^6.0.1"
  }
}
